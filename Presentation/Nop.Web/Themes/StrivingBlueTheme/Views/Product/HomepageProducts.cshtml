@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;

@if (Model.Count > 0)
{
    <div class="featuredBox">
        <h4>@T("HomePage.Products")</h4>
        <div class="row">

            @*<div class="item-grid">*@
                @foreach (var item in Model)
                {
                    <div class="item-box col-md-3">
                        @Html.Partial("_ProductBox", item)
                    </div>
                }
            @*</div>*@
        </div>
    </div>
}